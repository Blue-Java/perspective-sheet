<!DOCTYPE html>
<html>

<head>
  <%- include( '../layout/head.ejs' ) %>
  <title>Technology Survey Historical Results</title>
</head>

<body>
  <header>
    <%- include( '../layout/header.ejs' ) %>
    <div id="redHeader">
      <div>
        <h1>Technology Exposure and Perspective Survey</h1>
        <h2>Historical Survey Results</h2>
      </div>
    </div>
  </header>
  <ul class="tableHeader">
    <li>
      <div>
        <h2>Course</h2>
        <h2>Link</h2>
        <h2>Submision Count</h2>
        <h2>Chart</h2>
      </div>
    </li>
    <% forms.forEach(form =>{ %>
    <li>
      <h2><%= form.title %></h2>
      <a href="<%= form.url%>"><%= form.url%></a>
      <h2> Submision Count: <%= form.count%></h2>
      <button type="submit">Close Survey</button>
    </li>
    <%});%>
  </ul>
  <%- include('../partials/createsurvey') %>
</body>

</html>