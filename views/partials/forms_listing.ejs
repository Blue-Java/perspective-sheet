<ul class="tableHeader">
  <% if(forms.length) { %>
    <li>
      <div>
        <h2>Course</h2>
        <h2>Link</h2>
        <h2>Submision Count</h2>
        <h2>Chart</h2>
      </div>
    </li>
  <% } %>
  <% forms.forEach( form => { %>
  <li>
    <h2><%= form.title %></h2>
    <a href="<%= form.url %>"><%= form.url %></a>
    <h2> Submision Count: <%= form.count %></h2>
    <form id="formSubmit" action="/admin/<%= form.id %>?_method=DELETE"  method="post">
      <button type="submit">Close Survey</button>
    </form>
  </li>
  <% }); %>
</ul>